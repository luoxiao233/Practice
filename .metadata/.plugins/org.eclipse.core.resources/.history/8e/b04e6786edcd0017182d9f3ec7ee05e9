package com.java.string;

public class Demo2_Constructor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		demo1();
	}

	static void demo1() {
		String s1 = new String();
		System.out.println(s1);     //默认为空字符串的
		
		String s2 = "";
		System.out.println(s2);   //同样为空字符串
	}

}
